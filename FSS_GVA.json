{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "UK FSS GVA relative to GDP",
    "subtitle": ["in %"],
    "color": "#122b39E6",
    "subtitleColor": "#676A86",
    "anchor": "start",
    "dx": 24,
    "fontSize": 14,
    "subtitleFontSize": 12,
    "subtitlePadding": 4,
    "offset": 0
  },
  "width": 450,
  "height": 300,
  "config": {
    "font": "Circular Std Book",
    "mark": {"line": {"interpolate": "linear"}},
    "view": {"stroke": "transparent", "width": 400, "height": 300},
    "range": {},
    "axisX": {
      "labelColor": "#122b39",
      "tickColor": "#122b39",
      "domainColor": "#122b39",
      "domainOpacity": 0.5,
      "gridOpacity": 0,
      "labelFont": "Circular Std Book",
      "labelAngle": 0,
      "labelAlign": "center",
      "labelFontSize": 11,
      "labelPadding": 5,
      "tickCount": 10,
      "tickSize": 0,
      "title": ""
    },
    "axisY": {
      "labelColor": "#122b39",
      "labelFont": "Circular Std Book",
      "tickColor": "#122b39",
      "domainColor": "#122b39",
      "gridColor": "#122b39",
      "gridDash": [1, 5],
      "gridOpacity": 0.5,
      "labelPadding": 5,
      "labelFontSize": 11,
      "domainOpacity": 0.5,
      "tickSize": 0,
      "title": null,
      "titleAlign": "left",
      "titleAngle": 0,
      "titleBaseline": "bottom",
      "titleColor": "#122b39",
      "titleOpacity": 0.9,
      "titleX": 0,
      "titleY": -7
    }
  },
  "layer": [
    {
      "mark": {"type": "line"},
      "encoding": {
        "x": {"field": "Year", "title": "", "type": "temporal"},
        "y": {
          "field": "Value",
          "title": "",
          "type": "quantitative"
        },
        "color": {
          "value": "#1f77b4"
        }
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": 5,
        "dy": -5
      },
      "transform": [
        {
          "filter": "datum.Year == 2022"
        }
      ],
      "encoding": {
        "text": {"field": "Value", "type": "quantitative"},
        "x": {"field": "Year", "type": "temporal"},
        "y": {"field": "Value", "type": "quantitative"},
        "color": {"value": "#1f77b4"}
      }
    }
  ],
  "data": {
    "url": "https://raw.githubusercontent.com/louisflamencourt/GrowthCoLab/refs/heads/main/FSS%20GVA.csv",
    "format": {
      "type": "csv"
    }
  }
}
